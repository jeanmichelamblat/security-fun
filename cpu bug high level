<script language="Javascript">
function Togglehottopicseditorschoice(theTab) {
    if (theTab == 'editorschoice') {
        $('#midcol_hottopics').hide();
        $('#midcol_editorschoice').show();
        $('#midcol_editorschoice_button').removeClass('darkgray theNonSelectedTab').addClass('red theSelectedTabBlack');
        $('#midcol_hottopics_button').removeClass('red theSelectedTabBlack').addClass('darkgray theNonSelectedTab');
    } else {
        $('#midcol_hottopics').show();
        $('#midcol_editorschoice').hide();
        $('#midcol_hottopics_button').removeClass('darkgray theNonSelectedTab').addClass('red theSelectedTabBlack');
        $('#midcol_editorschoice_button').removeClass('red theSelectedTabBlack').addClass('darkgray theNonSelectedTab');
    }
}
</script>


