$(document).ready( function() {

	var $sidebar        = $(".post-sidebar-inner"), 
		$window         = $(window),
		offset          = $sidebar.offset(),
		topPadding      = $('.post-topics-wrap').outerHeight() + 50;
		floatingHeight  = '',
		postBodyPad     = parseFloat( $('.post-body').css('padding-bottom') );

	$window.scroll(function() {
	    nextElementTop  = $('#post-cta').offset().top;
	    floatingHeight = $('.post-sidebar-inner').outerHeight();
	    
		if ($window.scrollTop() > offset.top) {
		    if (nextElementTop - $window.scrollTop() - (postBodyPad*2) > floatingHeight) {
    			$sidebar.stop().animate({
    				top: $window.scrollTop() - offset.top + topPadding + 30
    			});
		    } else {
		        $sidebar.stop().animate({
    				top: nextElementTop - floatingHeight - (postBodyPad*2) - $('.post-body').offset().top
		        });
		    }
		} else {
			$sidebar.stop().animate({
				top: topPadding
			});
		}
	});
	

});
</script></div>

